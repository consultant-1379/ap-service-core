<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="unqualified">
    <xs:simpleType name="syncRefName">
        <xs:restriction base="xs:string">
            <xs:pattern value="(DU-1-[1-2])|(PF-[1-8])|(PT-[1-8])|(IP-[1-2]-[1-2])|(GPS-1-1)|(GPSOUT-1-1)|(GE-1)|(HTPSI-1)" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="tuSyncRefIdName">
        <xs:restriction base="xs:string">
            <xs:pattern value="([1-2])" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="packetFrequencySyncRefIdName">
        <xs:restriction base="xs:string">
            <xs:pattern value="([1-8])" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="packetTimeSyncRefIdName">
        <xs:restriction base="xs:string">
            <xs:pattern value="([1-8])" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ipSyncRefIdName">
        <xs:restriction base="xs:string">
            <xs:pattern value="([1-2])" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="gpsSyncRefIdName">
        <xs:restriction base="xs:string">
            <xs:pattern value="(1)" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="gpsOutSyncLinkIdName">
        <xs:restriction base="xs:string">
            <xs:pattern value="(1)" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="transportLayerRefName">
        <xs:restriction base="xs:string">
            <xs:pattern value="(1)" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="hptsiSyncRefIdName">
        <xs:restriction base="xs:string">
            <xs:pattern value="(1)" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="YesOrNo">
        <xs:restriction base="xs:string">
            <xs:enumeration value="YES" />
            <xs:enumeration value="NO" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TrueOrFalse">
        <xs:restriction base="xs:string">
            <xs:enumeration value="TRUE" />
            <xs:enumeration value="FALSE" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ipv4oIpv6Type">
        <xs:restriction base="xs:string">
            <xs:pattern
                value="((((25[0-5])|(2[0-4][0-9])|([01]?[0-9]?[0-9]))\.){3}((25[0-5])|(2[0-4][0-9])|([01]?[0-9]?[0-9])))|(s*((([0-9A-Fa-f]{1,4}:){7}([0-9A-Fa-f]{1,4}|:))|(([0-9A-Fa-f]{1,4}:){6}(:[0-9A-Fa-f]{1,4}|((25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3})|:))|(([0-9A-Fa-f]{1,4}:){5}(((:[0-9A-Fa-f]{1,4}){1,2})|:((25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3})|:))|(([0-9A-Fa-f]{1,4}:){4}(((:[0-9A-Fa-f]{1,4}){1,3})|((:[0-9A-Fa-f]{1,4})?:((25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3}))|:))|(([0-9A-Fa-f]{1,4}:){3}(((:[0-9A-Fa-f]{1,4}){1,4})|((:[0-9A-Fa-f]{1,4}){0,2}:((25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3}))|:))|(([0-9A-Fa-f]{1,4}:){2}(((:[0-9A-Fa-f]{1,4}){1,5})|((:[0-9A-Fa-f]{1,4}){0,3}:((25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3}))|:))|(([0-9A-Fa-f]{1,4}:){1}(((:[0-9A-Fa-f]{1,4}){1,6})|((:[0-9A-Fa-f]{1,4}){0,4}:((25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3}))|:))|(:(((:[0-9A-Fa-f]{1,4}){1,7})|((:[0-9A-Fa-f]{1,4}){0,5}:((25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3}))|:)))(%.+)?\s*)" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="routingSubnetMask">
        <xs:restriction base="xs:string">
            <xs:pattern
                value="((((25[0-5])|(2[0-4][0-9])|([01]?[0-9]?[0-9]))\.){3}((25[0-5])|(2[0-4][0-9])|([01]?[0-9]?[0-9])))|(s*((([0-9A-Fa-f]{1,4}:){7}([0-9A-Fa-f]{1,4}|:))|(([0-9A-Fa-f]{1,4}:){6}(:[0-9A-Fa-f]{1,4}|((25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3})|:))|(([0-9A-Fa-f]{1,4}:){5}(((:[0-9A-Fa-f]{1,4}){1,2})|:((25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3})|:))|(([0-9A-Fa-f]{1,4}:){4}(((:[0-9A-Fa-f]{1,4}){1,3})|((:[0-9A-Fa-f]{1,4})?:((25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3}))|:))|(([0-9A-Fa-f]{1,4}:){3}(((:[0-9A-Fa-f]{1,4}){1,4})|((:[0-9A-Fa-f]{1,4}){0,2}:((25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3}))|:))|(([0-9A-Fa-f]{1,4}:){2}(((:[0-9A-Fa-f]{1,4}){1,5})|((:[0-9A-Fa-f]{1,4}){0,3}:((25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3}))|:))|(([0-9A-Fa-f]{1,4}:){1}(((:[0-9A-Fa-f]{1,4}){1,6})|((:[0-9A-Fa-f]{1,4}){0,4}:((25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3}))|:))|(:(((:[0-9A-Fa-f]{1,4}){1,7})|((:[0-9A-Fa-f]{1,4}){0,5}:((25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)(\.(25[0-5]|2[0-4]\d|1\d\d|[1-9]?\d)){3}))|:)))(%.+)?\s*)" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="DU1orDU2">
        <xs:restriction base="xs:string">
            <xs:enumeration value="DU-1" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="primaryOrSecondary">
        <xs:restriction base="xs:string">
            <xs:enumeration value="primary" />
            <xs:enumeration value="secondary" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="activatedOrDeactivated">
        <xs:restriction base="xs:string">
            <xs:enumeration value="ACTIVATED" />
            <xs:enumeration value="DEACTIVATED" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="enabledOrDisabled">
        <xs:restriction base="xs:string">
            <xs:enumeration value="ENABLED" />
            <xs:enumeration value="DISABLED" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="bypassOrDrop">
        <xs:restriction base="xs:string">
            <xs:enumeration value="BYPASS" />
            <xs:enumeration value="DROP" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="protocolType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="ANY" />
            <xs:enumeration value="ICMP" />
            <xs:enumeration value="TCP" />
            <xs:enumeration value="UDP" />
            <xs:enumeration value="SCTP" />
            <xs:enumeration value="RAWIP" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="diffieHellmanGroupType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="GROUP_2" />
            <xs:enumeration value="GROUP_14" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ikePeerEncryptionAlgorithm">
        <xs:restriction base="xs:string">
            <xs:enumeration value="AES_CBC_128" />
            <xs:enumeration value="AES_CBC_256" />
            <xs:enumeration value="ALG_3DES_CBC" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ikePeerIntegrityAlgorithm">
        <xs:restriction base="xs:string">
            <xs:enumeration value="AES_XCBC_MAC_96" />
            <xs:enumeration value="HMAC_MD5_96" />
            <xs:enumeration value="HMAC_SHA_1_96" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="pseudoRandomFunctionType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="AES_XCBC_PRF128" />
            <xs:enumeration value="HMAC_SHA1" />
            <xs:enumeration value="HMAC_MD5" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="presharedKeyOrCertificate">
        <xs:restriction base="xs:string">
            <xs:enumeration value="PRESHARED_KEY" />
            <xs:enumeration value="CERTIFICATE" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="peerIdentityIdTypeEnum">
        <xs:restriction base="xs:string">
            <xs:enumeration value="ANY" />
            <xs:enumeration value="IP_V4_ADDRESS" />
            <xs:enumeration value="FQDN" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ipSecTunnelEncryptionAlgorithm">
        <xs:restriction base="xs:string">
            <xs:enumeration value="AES_CBC_128" />
            <xs:enumeration value="AES_CBC_256" />
            <xs:enumeration value="ALG_3DES_CBC" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ipSecTunnelIntegrityAlgorithm">
        <xs:restriction base="xs:string">
            <xs:enumeration value="AES_XCBC_MAC_96" />
            <xs:enumeration value="HMAC_SHA_1_96" />
            <xs:enumeration value="HMAC_MD5_96" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ipAddressOrMask">
        <xs:restriction base="xs:string">
            <xs:enumeration value="ipAddress" />
            <xs:enumeration value="mask" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="preSharedKeyEncodingEnum">
        <xs:restriction base="xs:string">
            <xs:enumeration value="HEX" />
            <xs:enumeration value="ASCII" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="OwnIdTypeEnum">
        <xs:restriction base="xs:string">
            <xs:enumeration value="AUTOMATIC" />
            <xs:enumeration value="IP_V4_ADDRESS" />
            <xs:enumeration value="FQDN" />
            <xs:enumeration value="AUTOMATIC_DN" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ConfigModeTypeEnum">
        <xs:restriction base="xs:string">
            <xs:enumeration value="MANUAL" />
            <xs:enumeration value="DHCP" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ConfModeTypeEnum">
        <xs:restriction base="xs:string">
            <xs:enumeration value="MANUAL" />
            <xs:enumeration value="IKE" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="IpVersionTypeEnum">
        <xs:restriction base="xs:string">
            <xs:enumeration value="IP_V4" />
            <xs:enumeration value="IP_V6" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="NodeIdTypeEnum">
        <xs:restriction base="xs:string">
            <xs:enumeration value="EMPTY" />
            <xs:enumeration value="FDN" />
            <xs:enumeration value="IP-Address" />
            <xs:enumeration value="HWID" />
        </xs:restriction>
    </xs:simpleType>
    <xs:element name="SiteBasic">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element name="Format">
                    <xs:annotation>
                        <xs:documentation>Contains information about the revision of the schema.</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:attribute name="revision" use="required">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="K" />
                                    <xs:enumeration value="D" />
                                    <xs:enumeration value="J" />
                                    <xs:enumeration value="T" />
                                    <xs:enumeration value="U" />
                                    <xs:enumeration value="V" />
                                    <xs:enumeration value="W" />
                                    <xs:enumeration value="X" />
                                    <xs:enumeration value="Y" />
                                    <xs:enumeration value="Z" />
                                    <xs:enumeration value="AA" />
                                    <xs:enumeration value="AB" />
                                    <xs:enumeration value="AC" />
                                    <xs:enumeration value="AD" />
                                    <xs:enumeration value="AE" />
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:attribute>
                    </xs:complexType>
                </xs:element>
                <xs:element name="Ip">
                    <xs:complexType>
                        <xs:choice maxOccurs="unbounded">
                            <xs:element name="IpRoutingTable" maxOccurs="1">
                                <xs:complexType>
                                    <xs:choice>
                                        <xs:element name="StaticRoutes" minOccurs="0" maxOccurs="200">
                                            <xs:complexType>
                                                <xs:attribute name="ipAddress" type="ipv4oIpv6Type" use="required" />
                                                <xs:attribute name="networkMask" type="routingSubnetMask" use="required" />
                                                <xs:attribute name="nextHopIpAddr" type="ipv4oIpv6Type" use="required" />
                                                <xs:attribute name="redistribute" type="TrueOrFalse" use="required" />
                                                <xs:attribute name="routeMetric" type="xs:int" use="required" />
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:choice>
                                    <xs:attribute name="userLabel" type="xs:string" default="" />
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="IpHostLink" maxOccurs="1">
                                <xs:complexType>
                                    <xs:attribute name="ipAddress" type="ipv4oIpv6Type" use="optional" />
                                    <xs:attribute name="ipHostLinkId" type="xs:string" use="required" />
                                    <xs:attribute name="ipInterfaceMoRef" type="xs:string" use="required" />
                                    <xs:attribute name="userLabel" type="xs:string" default="" />
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="oaMAlternativeAccessSource" minOccurs="0">
                                <xs:complexType>
                                    <xs:attribute name="ipAddress" type="ipv4oIpv6Type" use="required" />
                                    <xs:attribute name="networkMask" type="ipv4oIpv6Type" use="required" />
                                </xs:complexType>
                            </xs:element>
                        </xs:choice>
                        <xs:attribute name="dscp" use="optional">
                            <xs:simpleType>
                                <xs:restriction base="xs:int">
                                    <xs:minInclusive value="0" />
                                    <xs:maxInclusive value="63" />
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="dnsServer1" type="ipv4oIpv6Type" use="optional" />
                        <xs:attribute name="dnsServer2" type="ipv4oIpv6Type" use="optional" />
                        <xs:attribute name="dnsServer3" type="ipv4oIpv6Type" use="optional" />
                        <xs:attribute name="oaMAlternativeAccess" type="TrueOrFalse" use="optional" />
                    </xs:complexType>
                </xs:element>
                <xs:element name="ENodeBFunction">
                    <xs:annotation>
                        <xs:documentation>UpIpAccessHostEtRef is mandatory if more than one IpAccessHostEt is configured on the node, i.e. if IpSec is
                            used.
                        </xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:choice maxOccurs="unbounded">
                            <xs:element name="RbsConfiguration">
                                <xs:complexType>
                                    <xs:choice maxOccurs="1">
                                        <xs:element name="WantedPosition" minOccurs="0" maxOccurs="1">
                                            <xs:complexType>
                                                <xs:attribute name="latitude" type="xs:string" use="required" />
                                                <xs:attribute name="longitude" type="xs:string" use="required" />
                                                <xs:attribute name="altitude" type="xs:string" use="required" />
                                                <xs:attribute name="tolerance" type="xs:string" use="optional" />
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:choice>
                                    <xs:attribute name="ossCorbaNameServer" type="xs:string" use="required" />
                                    <xs:attribute name="nodeIdType" type="NodeIdTypeEnum" use="optional" default="FDN" />
                                    <xs:attribute name="nodeIdValue" type="xs:string" use="optional" default="" />
                                </xs:complexType>
                            </xs:element>
                        </xs:choice>
                        <xs:attribute name="upIpAccessHostRef" type="xs:string" use="optional" />
                        <xs:attribute name="timeAndPhaseSynchCritical" type="TrueOrFalse" use="optional" />
                        <xs:attribute name="timePhaseMaxDeviation" use="optional">
                            <xs:simpleType>
                                <xs:restriction base="xs:int">
                                    <xs:minInclusive value="9" />
                                    <xs:maxInclusive value="100" />
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:attribute>
                    </xs:complexType>
                </xs:element>
                <xs:element name="ManagedElementData" minOccurs="0">
                    <xs:complexType>
                        <xs:attribute name="documentServerAddress" type="xs:string" default="" />
                        <xs:attribute name="ntpServerAddressPrimary" type="ipv4oIpv6Type" use="optional" />
                        <xs:attribute name="ntpServiceActivePrimary" type="TrueOrFalse" default="FALSE" />
                        <xs:attribute name="ntpServerAddressSecondary" type="ipv4oIpv6Type" use="optional" />
                        <xs:attribute name="ntpServiceActiveSecondary" type="TrueOrFalse" default="FALSE" />
                        <xs:attribute name="ntpServerAddressThird" type="ipv4oIpv6Type" use="optional" />
                        <xs:attribute name="ntpServiceActiveThird" type="TrueOrFalse" default="FALSE" />
                        <xs:attribute name="nodeLocalTimeZone" type="xs:string" use="optional" />
                        <xs:attribute name="daylightSavingTime" type="TrueOrFalse" default="FALSE" />
                        <xs:attribute name="swServerUrlList" type="xs:string" use="optional" />
                        <xs:attribute name="docLibUserDefined" type="xs:string" default="" />
                    </xs:complexType>
                </xs:element>
                <xs:element name="IpSystem">
                    <xs:complexType>
                        <xs:choice maxOccurs="unbounded">
                            <xs:element name="IpAccessHostEt" maxOccurs="64">
                                <xs:complexType>
                                    <xs:choice maxOccurs="unbounded">
                                        <xs:element name="IpSyncRef" minOccurs="0" maxOccurs="8">
                                            <xs:complexType>
                                                <xs:attribute name="ipSyncRefId" type="ipSyncRefIdName" use="required" />
                                                <xs:attribute name="ntpServerIpAddress" type="ipv4oIpv6Type" use="required" />
                                                <xs:attribute name="userLabel" type="xs:string" default="" />
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:choice>
                                    <xs:attribute name="ipAccessHostEtId" type="xs:string" use="required" />
                                    <xs:attribute name="ipAddress" type="ipv4oIpv6Type" use="optional" />
                                    <xs:attribute name="ipInterfaceMoRef" type="xs:string" use="required" />
                                    <xs:attribute name="userLabel" type="xs:string" default="" />
                                    <xs:attribute name="networkPrefixLength" default="64">
                                        <xs:simpleType>
                                            <xs:restriction base="xs:int">
                                                <xs:minInclusive value="1" />
                                                <xs:maxInclusive value="128" />
                                            </xs:restriction>
                                        </xs:simpleType>
                                    </xs:attribute>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="VpnInterface" minOccurs="0">
                                <xs:complexType>
                                    <xs:attribute name="ipVersion" type="IpVersionTypeEnum" default="IP_V4" />
                                    <xs:attribute name="vpnInterfaceId" type="xs:string" default="1" />
                                    <xs:attribute name="configurationMode" type="ConfModeTypeEnum" default="MANUAL" />
                                    <xs:attribute name="ipAccessHostEtRef" type="xs:string" use="required" />
                                    <xs:attribute name="networkPrefixLength" use="optional">
                                        <xs:simpleType>
                                            <xs:restriction base="xs:int">
                                                <xs:minInclusive value="1" />
                                                <xs:maxInclusive value="31" />
                                            </xs:restriction>
                                        </xs:simpleType>
                                    </xs:attribute>
                                    <xs:attribute name="userLabel" type="xs:string" default="" />
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="IpSec" minOccurs="0">
                                <xs:complexType>
                                    <xs:choice maxOccurs="unbounded">
                                        <xs:element name="IkePeer" minOccurs="0" maxOccurs="unbounded">
                                            <xs:complexType>
                                                <xs:choice maxOccurs="unbounded">
                                                    <xs:element name="AllowedTransforms" minOccurs="0" maxOccurs="5">
                                                        <xs:complexType>
                                                            <xs:attribute name="diffieHellmanGroup" type="diffieHellmanGroupType" use="required" />
                                                            <xs:attribute name="encryptionAlgorithm" type="ikePeerEncryptionAlgorithm"
                                                                use="required" />
                                                            <xs:attribute name="integrityAlgorithm" type="ikePeerIntegrityAlgorithm" use="required" />
                                                            <xs:attribute name="pseudoRandomFunction" type="pseudoRandomFunctionType"
                                                                use="required" />
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:choice>
                                                <xs:attribute name="authenticationMethod" type="presharedKeyOrCertificate" default="CERTIFICATE" />
                                                <xs:attribute name="childSaLifeTimeDataLimit" default="50000">
                                                    <xs:simpleType>
                                                        <xs:restriction base="xs:int">
                                                            <xs:minInclusive value="100" />
                                                            <xs:maxInclusive value="500000" />
                                                        </xs:restriction>
                                                    </xs:simpleType>
                                                </xs:attribute>
                                                <xs:attribute name="childSaLifeTime" default="1440">
                                                    <xs:simpleType>
                                                        <xs:restriction base="xs:int">
                                                            <xs:minInclusive value="60" />
                                                            <xs:maxInclusive value="5000" />
                                                        </xs:restriction>
                                                    </xs:simpleType>
                                                </xs:attribute>
                                                <xs:attribute name="ikePeerId" type="xs:string" use="required" />
                                                <xs:attribute name="ikeSaLifeTime" type="xs:int" default="1440" />
                                                <xs:attribute name="logging" type="enabledOrDisabled" default="DISABLED" />
