<rpc xmlns="urn:ietf:params:xml:ns:netconf:base:1.0"
     message-id="Activation of key transport licensed features needed for connectivity with the OSS. Remaining licenses should be activated in a later stage">
   <edit-config>
      <target>
         <running/>
      </target>
      <config>
         <ManagedElement xmlns="urn:com:ericsson:ecim:ComTop">
            <managedElementId>1</managedElementId>
            <SystemFunctions>
               <systemFunctionsId>1</systemFunctionsId>
               <Lm xmlns="urn:com:ericsson:ecim:RcsLM">
                  <lmId>1</lmId>
                  <!--Include appropriate license activations below if needed. Note that the license for 10 G port capabilty is an HWAC license-->
                  <!--does not need be activated. However it must be present in the LKF file if 10G capability is needed.CXC number for critical transport features:-->
                  <!--Virtual router: CXC4011823, IPv6:CXC4040006, IPsec:CXC4040004, BFD: CXC4011707, Link aggregation:CXC4040002  -->
                  <!--VR license is always included in RBS configurations and can be activated-->
            
                  <FeatureState>
                     <featureStateId>CXC4011823</featureStateId>
                     <featureState>ACTIVATED</featureState>
                  </FeatureState>
            
               </Lm>
            </SystemFunctions>
         </ManagedElement>
      </config>
   </edit-config>
</rpc>
]]>]]>

<rpc xmlns="urn:ietf:params:xml:ns:netconf:base:1.0"
     message-id="Create Transport configuration. This includes all the MOs contained under the Transport MO and the TnPort under Equipment">
   <edit-config>
      <target>
         <running/>
      </target>
      <config>
         <ManagedElement xmlns="urn:com:ericsson:ecim:ComTop">
            <managedElementId>1</managedElementId>
            <!--- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
            <!--- - - - - - - - - - - - - - -Create TnPort's - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
            <!--- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
            <Equipment>
               <equipmentId>1</equipmentId>
               <FieldReplaceableUnit>
                  <fieldReplaceableUnitId>BB-1</fieldReplaceableUnitId>
                  <TnPort>
                     <tnPortId>TN_A</tnPortId>
                  </TnPort>
                  <SyncPort>
                     <syncPortId>SYNC</syncPortId>
                  </SyncPort>
               </FieldReplaceableUnit>
            </Equipment>
            <Transport>
               <transportId>1</transportId>
               <!--- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
               <!--Configure Ethernet ports-->
               <!--- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
               <!--- - - - - -Ethernet port for TN_A - - - - - - -  -->
               <EthernetPort xmlns="urn:com:ericsson:ecim:RtnL2EthernetPort">
                  <ethernetPortId>TN_A</ethernetPortId>
                  <administrativeState>UNLOCKED</administrativeState>
                  <encapsulation>ManagedElement=1,Equipment=1,FieldReplaceableUnit=BB-1,TnPort=TN_A</encapsulation>
                  <admOperatingMode>ANY</admOperatingMode>
               </EthernetPort>
               <!--- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
               <!--Configure Routers and VlanPorts- -->
               <!--- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
               <!--- - - - - - - - VlanPort towards port TN_A used by Router: OAM-->
               <VlanPort xmlns="urn:com:ericsson:ecim:RtnL2VlanPort">
                  <vlanPortId>OAM.IF1</vlanPortId>
                  <encapsulation>ManagedElement=1,Transport=1,EthernetPort=TN_A</encapsulation>
                  <vlanId>192</vlanId>
                  <isTagged>true</isTagged>
               </VlanPort>
               <!--- - - - - Create Router  and all it's contained objects - - - - - -->
               <Router xmlns="urn:com:ericsson:ecim:RtnL3Router">
                  <routerId>OAM</routerId>
                  <InterfaceIPv4 xmlns="urn:com:ericsson:ecim:RtnL3InterfaceIPv4">
                     <interfaceIPv4Id>1</interfaceIPv4Id>
                     <AddressIPv4 xmlns="urn:com:ericsson:ecim:RtnL3AddressIPv4">
                        <addressIPv4Id>1</addressIPv4Id>
                        <address>10.186.192.129/24</address>
                     </AddressIPv4>
                     <encapsulation>ManagedElement=1,Transport=1,VlanPort=OAM.IF1</encapsulation>
                  </InterfaceIPv4>
                  <RouteTableIPv4Static xmlns="urn:com:ericsson:ecim:RtnRoutesStaticRouteIPv4">
                     <routeTableIPv4StaticId>1</routeTableIPv4StaticId>
                     <Dst>
                        <dstId>default</dstId>
                        <NextHop>
                           <nextHopId>1</nextHopId>
                           <address>10.186.139.254</address>
                        </NextHop>
                        <dst>0.0.0.0/0</dst>
                     </Dst>
                  </RouteTableIPv4Static>
               </Router>
               <!--- - - - - - - - VlanPort towards port TN_A used by Router: LTE-->
               <VlanPort xmlns="urn:com:ericsson:ecim:RtnL2VlanPort">
                  <vlanPortId>LTE.IF1</vlanPortId>
                  <encapsulation>ManagedElement=1,Transport=1,EthernetPort=TN_A</encapsulation>
                  <vlanId>139</vlanId>
                  <isTagged>true</isTagged>
               </VlanPort>
               <!--- - - - - Create Router  and all it's contained objects - - - - - -->
               <Router xmlns="urn:com:ericsson:ecim:RtnL3Router">
                  <routerId>LTE</routerId>
                  <InterfaceIPv4 xmlns="urn:com:ericsson:ecim:RtnL3InterfaceIPv4">
                     <interfaceIPv4Id>1</interfaceIPv4Id>
                     <AddressIPv4 xmlns="urn:com:ericsson:ecim:RtnL3AddressIPv4">
                        <addressIPv4Id>1</addressIPv4Id>
                        <address>10.186.192.129/24</address>
                     </AddressIPv4>
                     <encapsulation>ManagedElement=1,Transport=1,VlanPort=LTE.IF1</encapsulation>
                  </InterfaceIPv4>
                  <RouteTableIPv4Static xmlns="urn:com:ericsson:ecim:RtnRoutesStaticRouteIPv4">
                     <routeTableIPv4StaticId>1</routeTableIPv4StaticId>
                     <Dst>
                        <dstId>default</dstId>
                        <NextHop>
                           <nextHopId>1</nextHopId>
                           <address>10.186.139.254</address>
                        </NextHop>
                        <dst>0.0.0.0/0</dst>
                     </Dst>
                  </RouteTableIPv4Static>
               </Router>
               <!--- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
               <!--Configure Synchronization- - - - - - - -->
               <!--- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
               <Synchronization xmlns="urn:com:ericsson:ecim:RsyncSyncronization">
                  <synchronizationId>1</synchronizationId>
                  <telecomStandard>OPTION_I</telecomStandard>
                  <TimeSyncIO>
                     <timeSyncIOId>GPS</timeSyncIOId>
                     <encapsulation>ManagedElement=1,Equipment=1,FieldReplaceableUnit=BB-1,SyncPort=SYNC</encapsulation>
                  </TimeSyncIO>
                  <RadioEquipmentClock xmlns="urn:com:ericsson:ecim:RsyncRadioEquipmentClock">
                     <radioEquipmentClockId>1</radioEquipmentClockId>
                     <selectionProcessMode>QL_ENABLED</selectionProcessMode>
                     <minQualityLevel/>
                     <RadioEquipmentClockReference>
                        <radioEquipmentClockReferenceId>GPS</radioEquipmentClockReferenceId>
                        <administrativeState>UNLOCKED</administrativeState>
                        <priority>1</priority>
                        <adminQualityLevel/>
                        <encapsulation>ManagedElement=1,Transport=1,Synchronization=1,TimeSyncIO=GPS</encapsulation>
                     </RadioEquipmentClockReference>
                  </RadioEquipmentClock>
               </Synchronization>
            </Transport>
         </ManagedElement>
      </config>
   </edit-config>
</rpc>
]]>]]>

